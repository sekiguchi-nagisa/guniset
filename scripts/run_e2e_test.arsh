#!/usr/bin/env arsh

test -x $1 || { echo 1>&2 require guniset executable: $1; exit 1; }
let bin = $1.realpath()

importenv GUNISET_DIR : "$SCRIPT_DIR/.."
$GUNISET_DIR = $GUNISET_DIR.realpath()

cd $SCRIPT_DIR/..

shctl set errraise

./scripts/run_e2e_generate_test.arsh $bin
./scripts/run_e2e_enum_test.arsh $bin